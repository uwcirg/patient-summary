{
  "name": "patient-summary",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@material-table/core": "^0.2.53",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.88",
    "@mui/material": "^5.8.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "buffer": "^6.0.3",
    "commonmark": "^0.30.0",
    "cql-exec-fhir": "^2.0.2",
    "cql-exec-vsac": "^1.2.2",
    "cql-worker": "^1.1.4",
    "dayjs": "^1.11.6",
    "dompurify": "^3.2.4",
    "fhirclient": "^2.4.0",
    "fs": "0.0.1-security",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.3.0",
    "recharts": "^2.1.13",
    "sass": "^1.53.0"
  },
  "scripts": {
    "start": "vite --trace-warnings",
    "build": "vite build",
    "serve": "vite preview",
    "test": "rescripts test --testPathIgnorePatterns=./src/__tests__/cql --watchAll=false",
    "test:watch": "rescripts test --testPathIgnorePatterns=./src/__tests__/cql",
    "cql-test": "./node_modules/.bin/mocha ./src/__tests__/cql/*.js --recursive",
    "eject": "rescripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.20.2",
    "@vitejs/plugin-react": "^4.3.4",
    "cql-execution": "^2.4.1",
    "cql-testing": "^2.5.2",
    "mocha": "^10.0.0",
    "node-fetch": "^2.6.7",
    "vite": "^6.2.1",
    "vite-plugin-babel": "^1.3.0",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-plugin-node-polyfills": "^0.23.0",
    "worker-loader": "^3.0.8"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "cql-worker/"
    ],
    "moduleNameMapper": {
      "^cql*": "<rootDir>/src/__tests__/mockfiles/Worker.js"
    },
    "testMatch": [
      "<rootDir>/src/__tests__/components/*.js"
    ]
  }
}
